  [i:263, score:0.52716] /*重複条件付きドキュメント頻度を一斉に求める関数結果は，staticな配列に保存する．
  [i:264, score:0.38414] */ static int dfn[MAX_C]; static void count_dfn(char *s, int len) { int i; /* string position */ int t; /* term frequency in a document */ int n; /* document frequency */ int k; n = 0; t = 0; for(k=0;k<MAX_C;k++) { dfn[k]=0; }; for(i=0;i<size;i++) { if(strncmp(&text[i], s, len)==0) t++; if(text[i]=='\n') { for(k=0;k<MAX_C;k++) { if(t>k) dfn[k]++; } t = 0; } } }
